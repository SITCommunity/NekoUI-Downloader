{
  "schema_version": 1,

  "quilt_loader": {
    "group": "org.brokenedtz.nekoui",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "NekoUI: Java Edition Introduces Custom User Interface (CUI), Blue Archive Background in Minecraft: Java Edition",

      "contributors": {
        "brokenedtz": "Dev"
      },

      "contact": {
        "homepage": "https://modrinth.com/mod/nekoui",
        "issues": "https://github.com/CyraTeam/NekoUI-Download/issues"
      },

      "license": "CC-BY-NC-ND-4.0",
      "icon": "${mod_id}.png"
    },

    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "org.brokenedtz.nekoui.NekoUIQuilt"
      ]
    },

    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.19.1"
      },

      {
        "id": "java",
        "versions": ">=17"
      },

      {
        "id": "minecraft",
        "versions": "${minecraft_version}"
      }
    ]
  }
}